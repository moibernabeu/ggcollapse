filter(prop_KOs_in_LECA >= prop_KOs_in_LECA_threshold)
cat(kept_dons$V2, sep = '\n', file = paste0('../outputs/donor_descendants/', don, '.txt'))
donor_descendants[[don]] <- kept_dons$V2
}
